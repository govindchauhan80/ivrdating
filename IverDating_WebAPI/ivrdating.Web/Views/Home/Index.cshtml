    <div class="col-md-3">
        <ul>
            @foreach (var item in ViewBag.Files)
            {
                <li><a href="~/Log/@item" download="" class="btn btn-primary btn-lg btnHeight">Download @item</a></li>
            }
        </ul>

    </div>
</div>
